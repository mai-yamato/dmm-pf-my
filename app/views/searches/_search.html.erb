    <div class="pb-5">
      <div class="search_img">
        <h3>検索⤵</h3>
        <%= form_with url: search_path, local: true, method: :get do |f| %>
          <%= f.text_field :word %>
          <%= f.select :range, options_for_select(['Temple']) %>
          <%= f.select :search, options_for_select([["部分一致","partial_match"], ["完全一致","perfect_match"]]) %>
          <%= f.submit "検索", class: "btn btn-border border-primary" %>
        <% end %>
  
        <table class="table table-hover table-inverse">
          <tbody>
            <% search_result_temple.each do |temple| %>
              <tr>
              <td><%= temple.temple_name %></td>
              <td><%= temple.opinion %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>